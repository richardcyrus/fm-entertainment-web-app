{
  "root": true,
  "extends": [
    "next/core-web-vitals",
    "plugin:jest-dom/recommended",
    "prettier"
  ],
  "plugins": ["testing-library"],
  "overrides": [
    {
      "files": [
        "**/__tests__/**/*.[jt]s?(x)",
        "**/?(*.)+(spec|test).[jt]s?(x)"
      ],
      "extends": ["plugin:testing-library/react"]
    }
  ]
}
